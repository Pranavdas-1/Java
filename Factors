import java.util.Scanner;
class factor
{
	public static void main(String args[])
	{
		int n,i,flag=0;

		System.out.print("Enter the number");

		Scanner add=new Scanner(System.in);

		n=add.nextInt();

		System.out.print("The Factors of "+n+"is");

		for(i=1;i<=n;i++)
		{
			if(n%i==0)
			{
				System.out.print("\t"+i);
			}
		}
	}
}
